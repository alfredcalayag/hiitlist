<!-- List: show.html.erb -->
<header class="workout_title">
  <h2><%= @list.name %> Workout</h2>
</header>

<aside class="duration">
  <ul>
    <li>High Intensity Duration: 00:<%= @list.high_time %></li>
    <li>Rest Duration: 00:<%= @list.low_time %></li>
  </ul>
</aside>
<%= render partial: "partials/add_exercise_form", object: @list %>
<%= render partial: "partials/playlist", object: @exercises %>

<button type="button" class="purple-btn" id="start"><h4>BEGIN HIIT</h4></button>

<!-- Workout Screen -->
<%= render "partials/workout" %>


<audio id="bell" src="<%= audio_path("bell.mp3") %>"></audio>
<audio id="gong" src="<%= audio_path("gong.mp3") %>"></audio>


<%= javascript_include_tag "audio" %>
<%= javascript_include_tag "timer" %>